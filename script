local UserInputService = game:GetService("UserInputService")
local sonicmaybe = 100
local istweenin = false
local gui = asdfghjgfhjdkghfjffghdjhjsdhfksdjhfasdjkfhasdklfhasdjkfhasklqwweett
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

gui.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
		dragging = true
		dragStart = input.Position
		startPos = gui.Position

		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

gui.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
		dragInput = input
	end
end)

UserInputService.InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

local button = slow

local function onButtonActivated()
	sonicmaybe = 75
end

button.Activated:Connect(onButtonActivated)
local button2 = normal

local function onButtonActivated2()
	sonicmaybe = 125
end

button2.Activated:Connect(onButtonActivated2)
local button3 = fast

local function onButtonActivated3()
	sonicmaybe = 400
end

button3.Activated:Connect(onButtonActivated3)
local button4 = lucidrious_speed

local function onButtonActivated4()
	sonicmaybe = 800
end

button4.Activated:Connect(onButtonActivated4)
local startbutton = go

local function onButtonActivated6()
	if istweenin == false then
		istweenin = true
	else
		istweenin = false
	end
end

startbutton.Activated:Connect(onButtonActivated6)
local closebutton = close

local function onButtonActivatedBye()
	asdfghjgfhjdkghfjffghdjhjsdhfksdjhfasdjkfhasdklfhasdjkfhasklqwweest:Destroy() -- thanos snap ui when closed
end

closebutton.Activated:Connect(onButtonActivatedBye)
local speed = sonicmaybe -- set this lower to make it slower

local Imput = game:GetService("UserInputService")
local Plr = game.Players.LocalPlayer
local Mouse = Plr:GetMouse()

function To(position)
	local Chr = Plr.Character
	if Chr ~= nil then
		local ts = game:GetService("TweenService")
		local char = game.Players.LocalPlayer.Character
		local hm = char.HumanoidRootPart
		local dist = (hm.Position - Mouse.Hit.p).magnitude
		local tweenspeed = dist/tonumber(speed)
		local ti = TweenInfo.new(tonumber(tweenspeed), Enum.EasingStyle.Linear)
		local tp = {CFrame = CFrame.new(position)}
		local tweenabc = ts:Create(hm, ti, tp)
		tweenabc:Play()
		if hm.Position == tp then
			tweenabc:Stop() -- important, this is for a bug fix.
		end
	end
end

Imput.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 and Imput:IsKeyDown(Enum.KeyCode.LeftControl) and istweenin == true then
		To(Mouse.Hit.p)
	end
end)
